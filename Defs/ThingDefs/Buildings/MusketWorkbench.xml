<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="WorkTable">
    <defName>MusketBench</defName>
    <label>Musket workbench</label>
    <description>A table for the assembling early firearms, muskets, rifled muskets.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Buildings/musketBench/mskbench</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(2,1)</drawSize>
      <shadowData>
        <volume>(1.7, 0.5, 0.7)</volume>
        <offset>(0,0,0)</offset>
      </shadowData>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsProduction</li>
    </thingCategories>
    <statBases>
      <WorkToBuild>3200</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <WorkTableWorkSpeedFactor>0.6</WorkTableWorkSpeedFactor>
      <OutdoorWorkRateFactor>0.6</OutdoorWorkRateFactor>
      <Cleanliness>-6</Cleanliness>
      <Mass>350</Mass>
    </statBases>
    <designationCategory>Production</designationCategory>
    <size>(2,1)</size>
    <stuffCategories>
      <li>Woody</li>
      <li>Metallic</li>
      <li>Stony</li>
      <li>Bricks</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <costList>
      <ComponentMedieval>8</ComponentMedieval>
      <TFJ_Tool_Building_Hammer>1</TFJ_Tool_Building_Hammer>
      <Mechanism>4</Mechanism>
    </costList>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <tickerType>Normal</tickerType>
    <rotatable>true</rotatable>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>Small_ToolCabinet</li>
        </linkableFacilities>
      </li>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <WorkTableWorkSpeedFactor>0.10</WorkTableWorkSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
      </li>
    </comps>
    <building>
      <uninstallWork>1450</uninstallWork>
      <sowTag>SupportPlantsOnly</sowTag>
      <canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
      <ai_chillDestination>false</ai_chillDestination>
      <artificialForMeditationPurposes>false</artificialForMeditationPurposes>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
    </building>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
      <li>PlaceWorker_ReportWorkSpeedPenalties</li>
    </placeWorkers>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
    <researchPrerequisites>
      <li>AF_Research_EarlyExperiments</li>
    </researchPrerequisites>
  </ThingDef>
</Defs>